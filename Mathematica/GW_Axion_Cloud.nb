      RowBox[{"(", 
       RowBox[{"r", "-", 
        RowBox[{"I", " ", "a", " ", 
         RowBox[{"Cos", "[", "\[Theta]", "]"}]}]}], ")"}]}]], 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"-", "I"}], " ", "a", " ", 
