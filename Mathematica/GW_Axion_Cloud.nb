Cell[BoxData[{
 RowBox[{
  RowBox[{"<<", "Teukolsky`"}], " ", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{
    "before", " ", "running", " ", "make", " ", "sure", " ", "you", " ", 
     "have", " ", "the", " ", "Teukolsky", " ", "package", " ", "from", " ", 
     "the", " ", "BHPT", " ", 
     RowBox[{"installed", ":", " ", "https", ":"}]}], "//", 
    RowBox[{
     RowBox[{
      RowBox[{"bhptoolkit", ".", "org"}], "/", "Teukolsky"}], "/"}]}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"$MinPrecision", "=", "60"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"INTBHPT", "[", 
    RowBox[{
