      ",", "Rmax_", ",", "NITMAX_", ",", "NITMAXS_"}], "]"}], ":=", 
   RowBox[{"(", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
      RowBox[{"doing", " ", "computations", " ", "for", " ", "l"}], "=", 
      RowBox[{"m", "=", "1"}]}], "*)"}], "\[IndentingNewLine]", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"scalar", "=", 
      RowBox[{"EIGENS", "[", 
       RowBox[{
       "a0", ",", "\[Mu]0", ",", "1", ",", "1", ",", "Rmax", ",", "NITMAXS"}],
        "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"\[Omega]R", "=", 
      RowBox[{"SetPrecision", "[", 
       RowBox[{
        RowBox[{"Re", "[", 
         RowBox[{"scalar", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "]"}], ",", "60"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"A0", "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{"A0c", "=", "1"}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"param", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"lS", "\[Rule]", "1"}], ",", 
        RowBox[{"mS", "\[Rule]", "1"}], ",", 
        RowBox[{"\[Omega]", "\[Rule]", 
         RowBox[{"2", 
          RowBox[{"Sign", "[", "m0", "]"}], 
          RowBox[{"Re", "[", "\[Omega]R", "]"}]}]}], ",", 
        RowBox[{"\[Omega]RS", "\[Rule]", "\[Omega]R"}], ",", 
        RowBox[{"l", "\[Rule]", "l0"}], ",", 
        RowBox[{"m", "\[Rule]", " ", "m0"}], ",", 
        RowBox[{"a", "\[Rule]", " ", "a0"}], ",", 
        RowBox[{"\[Mu]", "\[Rule]", " ", "\[Mu]0"}], ",", 
        RowBox[{
         RowBox[{"hh", "[", "0", "]"}], "\[Rule]", "1"}], ",", 
        RowBox[{"kk", "\[Rule]", "k0n"}], ",", 
        RowBox[{"k0", "\[Rule]", "k0n"}]}], "}"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"k0n", "=", 
      RowBox[{"I", " ", "\[Omega]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"s", "=", 
      RowBox[{"-", "2"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"rplus", "=", 
      RowBox[{"M", "+", 
       SqrtBox[
        RowBox[{
         SuperscriptBox["M", "2"], "-", 
         SuperscriptBox["a", "2"]}]]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"rh", "=", 
      RowBox[{"rplus", "//.", "param"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"rhn", "=", 
      RowBox[{"rh", 
       RowBox[{"(", 
        RowBox[{"1", "+", "EPSG"}], ")"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"INF", "=", 
      RowBox[{
       RowBox[{"Abs", "[", "INF0", "]"}], "//.", "param"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"density", "=", 
      RowBox[{"ParametricNDSolveValue", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"y", "'"}], "[", "\[Theta]", "]"}], "\[Equal]", 
           RowBox[{"2", "\[Pi]", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{"\[Rho]", "[", 
                 RowBox[{"r", ",", "\[Theta]"}], "]"}], " ", 
                RowBox[{"(", 
                 RowBox[{
                  SuperscriptBox["r", "2"], "+", 
                  RowBox[{
                   SuperscriptBox["a", "2"], " ", 
                   SuperscriptBox[
                    RowBox[{"Cos", "[", "\[Theta]", "]"}], "2"]}]}], ")"}], 
                " ", 
                RowBox[{"Sin", "[", "\[Theta]", "]"}]}], "//.", "param"}], "//.", 
