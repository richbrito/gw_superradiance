      RowBox[{
       RowBox[{"1", "/", "2"}], "*", 
       RowBox[{"Abs", "[", 
        RowBox[{"m", "+", "s"}], "]"}]}]}], ";", 
     RowBox[{"Alm", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"l", "*", 
